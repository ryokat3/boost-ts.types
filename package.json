{
  "name": "boost-ts.types",
  "version": "0.3.4",
  "description": "Typescript advanced type collection",
  "main": "index.ts",
  "scripts": {
    "test": "mocha 'test/**/*.ts'",
    "clean": "shx rm -rf dist tmp boost-ts.types*.tgz",
    "release": "shx mkdir dist && shx cp package.json README.md LICENSE src/* dist && cd dist && npm pack --pack-destination .."
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "*.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ryokat3/boost-ts.types.git"
  },
  "keywords": [
    "typescript",
    "types"
  ],
  "author": "ryokat3 <ryokat3@gmail.com> (https://github.com/ryokat3)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryokat3/boost-ts.types/issues"
  },
  "homepage": "https://github.com/ryokat3/boost-ts.types#readme",
  "publishConfig": {
    "access": "public"
  },
  "mocha": {
    "node-option": [
      "import=tsx"
    ]
  },
  "devDependencies": {
    "@types/chai": "^5.2.1",
    "@types/mocha": "^10.0.10",
    "chai": "^5.2.0",
    "mocha": "^11.1.0",
    "shx": "^0.4.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
